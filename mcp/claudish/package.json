{
  "name": "claudish",
  "version": "1.0.3",
  "description": "Run Claude Code with OpenRouter models via local proxy",
  "type": "module",
  "bin": {
    "claudish": "./dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:grok": "bun run src/index.ts --interactive --model x-ai/grok-code-fast-1",
    "build": "bun build src/index.ts --outdir dist --target bun --minify --external react --external ink --external react-devtools-core && chmod +x dist/index.js",
    "test": "bun test ./tests/comprehensive-model-test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "install": "bun run build && bun link"
  },
  "dependencies": {
    "ink": "^6.4.0",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "typescript": "^5.7.0"
  },
  "keywords": ["claude", "claude-code", "openrouter", "proxy", "cli", "ai"],
  "author": "Jack Rudenko <i@madappgang.com>",
  "license": "MIT"
}
